<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_services"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="0dp"
    android:paddingRight="0dp"
    android:paddingTop="0dp"
    tools:context="com.example.claudyanearj.aphv.MainActivity"
    android:background="@android:color/background_light">

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:padding="0dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginBottom="229dp">

        <ImageView
            android:layout_width="wrap_content"
            app:srcCompat="@drawable/hops"
            android:id="@+id/imageView3"
            android:layout_alignParentBottom="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_height="295dp" />

    </LinearLayout>

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="98dp"
        android:layout_height="98dp"
        android:layout_alignParentBottom="true"
        android:layout_alignRight="@+id/imageButton31"
        android:layout_alignEnd="@+id/imageButton31"
        android:layout_marginRight="20dp"
        android:layout_marginEnd="20dp">

    </LinearLayout>

    <ImageButton
        android:layout_width="98dp"
        android:layout_height="98dp"
        app:srcCompat="@drawable/lab"
        android:id="@+id/imageButton29"
        android:layout_weight="1"
        android:layout_marginBottom="56dp"
        android:layout_alignParentBottom="true"
        android:layout_alignLeft="@+id/imageButton27"
        android:layout_alignStart="@+id/imageButton27" />

    <ImageButton
        android:layout_width="98dp"
        android:layout_height="98dp"
        app:srcCompat="@drawable/q"
        android:id="@+id/imageButton31"
        android:layout_alignTop="@+id/imageButton29"
        android:layout_toRightOf="@+id/imageButton29"
        android:layout_toEndOf="@+id/imageButton29" />

    <ImageButton
        android:layout_width="98dp"
        android:layout_height="98dp"
        app:srcCompat="@drawable/cora"
        android:id="@+id/imageButton27"
        android:layout_above="@+id/imageButton29"
        android:layout_centerHorizontal="true" />

    <ImageButton
        android:layout_width="98dp"
        android:layout_height="98dp"
        app:srcCompat="@drawable/ca"
        android:id="@+id/imageButton28"
        android:layout_alignTop="@+id/imageButton27"
        android:layout_toRightOf="@+id/imageButton29"
        android:layout_toEndOf="@+id/imageButton29" />

    <ImageButton
        android:layout_width="98dp"
        android:layout_height="98dp"
        app:srcCompat="@drawable/cir"
        android:id="@+id/imageButton33"
        android:layout_alignTop="@+id/imageButton27"
        android:layout_alignLeft="@+id/imageButton25"
        android:layout_alignStart="@+id/imageButton25" />

    <ImageButton
        android:layout_width="98dp"
        android:layout_height="98dp"
        app:srcCompat="@drawable/diag"
        android:id="@+id/imageButton25"
        android:layout_below="@+id/imageButton27"
        android:layout_toLeftOf="@+id/imageButton27"
        android:layout_toStartOf="@+id/imageButton27" />

</RelativeLayout>
